# Environment
NODE_ENV=development

# Worker identification
WORKER_ID=worker-1
WORKER_CONCURRENCY=1
WORKER_SECRET=your-secret-token-here

# Redis connection (same as web app)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Web app connection
WEB_APP_URL=http://localhost:3000

# Output directories
OUTPUT_DIR=./output
SCREENSHOTS_DIR=./output/screenshots

# Public URL for serving files
PUBLIC_URL=http://localhost:3001/files

# Puppeteer settings
PUPPETEER_EXECUTABLE_PATH=
